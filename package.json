{
  "name": "unplugin-locator",
  "type": "module",
  "version": "0.1.1-beta.6",
  "packageManager": "pnpm@8.6.12",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/mahoushoujoarale/unplugin-locator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahoushoujoarale/unplugin-locator.git"
  },
  "bugs": {
    "url": "https://github.com/mahoushoujoarale/unplugin-locator/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/core/client.d.ts",
      "import": "./dist/core/client.js",
      "require": "./dist/core/client.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "demo:vite-preact": "npm -C demos/vite-preact run dev",
    "demo:vite-react": "npm -C demos/vite-react run dev",
    "demo:vite-vue": "npm -C demos/vite-vue run dev",
    "demo:vite-solid": "npm -C demos/vite-solid run dev",
    "demo:webpack-vue": "npm -C demos/webpack-vue run serve",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.12.16",
    "@popperjs/core": "^2.11.8",
    "@vue/compiler-dom": "^3.4.14",
    "launch-editor": "^2.6.1",
    "magic-string": "^0.30.5",
    "portfinder": "^1.0.32",
    "unplugin": "^1.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.2.2",
    "@babel/plugin-transform-typescript": "^7.23.6",
    "@nuxt/kit": "^3.8.2",
    "@nuxt/schema": "^3.8.2",
    "@types/node": "^20.10.3",
    "bumpp": "^9.2.0",
    "chalk": "^5.3.0",
    "eslint": "^8.55.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0"
  }
}
